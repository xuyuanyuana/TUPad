{
  "name": "tutupad",
  "version": "1.0.0",
  "description": "tutu and yuanyuan pad",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js",
